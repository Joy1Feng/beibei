	    -ms-flex-align: center;
	        align-items: center;
	font-size: 30rpx;
}
.flex-item>image {
	width: 100rpx;
	height: 100rpx;
}
.renwuchuli {
	padding: 0 33rpx;
	background-color: #fff;
	margin-top: 30rpx;
}
.renwuchuli_head {
	height: 80rpx;
	line-height: 80rpx;
	position: relative;
	border-bottom: 1px dashed #dcdcdc;
}
.renwuchuli_head:before {
	content: "";
	display: block;
	position: absolute;
	left: 0;
	top: 14rpx;
	height: 70%;
	width: 6rpx;
	background-color: #2b8cf9;
}
.renwuchuli_head .renwuchuli_head_title {
	font-size: 32rpx;
	margin-left: 24rpx;
	color: #666666;
}
.renwuchuli_head .more {
	float: right;
	font-size: 26rpx;
	color: #999999;
}
.box{
	padding: 0 33rpx;
	background-color: #fff;
}
.border_bottom{
	border-bottom: 1px solid #EEEEEE;
}

